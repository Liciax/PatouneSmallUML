<?xml version="1.0" encoding="UTF-8"?>
<smalluml:Diagram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:smalluml="http://www.univ-nantes.fr/smalluml"
    xsi:schemaLocation="http://www.univ-nantes.fr/smalluml /SmallUML/model/smalluml.ecore"
    name="OneDirection">
  <entities
      xsi:type="smalluml:Type"
      name="String"/>
  <entities
      xsi:type="smalluml:Type"
      name="Integer"/>
  <entities
      xsi:type="smalluml:Type"
      name="Boolean"/>
  <entities
      xsi:type="smalluml:Enumeration"
      name="InstrumentFamily">
    <elements>Wind</elements>
    <elements>Percussion</elements>
    <elements>Electronic</elements>
  </entities>
  <entities
      xsi:type="smalluml:Class"
      name="Member"
      abstract="true">
    <attributes
        name="name"
        type="//@entities.0"/>
    <attributes
        name="firstname"
        type="//@entities.0"/>
  </entities>
  <entities
      xsi:type="smalluml:Class"
      name="Singer"
      extends="//@entities.4">
    <operations
        name="sing"
        returnType="//@entities.0">
      <parameters
          name="musicSheet"
          type="//@entities.0"/>
    </operations>
    <operations
        name="twitterDrama"
        returnType="//@entities.0"/>
  </entities>
  <entities
      xsi:type="smalluml:Class"
      name="Musician"
      extends="//@entities.4">
    <attributes
        name="instrument"/>
    <operations
        name="play"
        returnType="//@entities.0">
      <parameters
          name="soundVolume"
          type="//@entities.1"/>
      <parameters
          name="musicSheet"
          type="//@entities.0"/>
    </operations>
  </entities>
  <entities
      xsi:type="smalluml:Class"
      name="Instrument">
    <attributes
        name="name"
        type="//@entities.0"/>
    <attributes
        name="family"
        type="//@entities.3"/>
  </entities>
  <entities
      xsi:type="smalluml:Class"
      name="PurityRing">
    <attributes
        name="violated"
        type="//@entities.2"/>
  </entities>
  <entities
      xsi:type="smalluml:Association"
      name="SacredOath">
    <roles name="member"
        entity="//@entities.4">
      <cardinality
          lowerBound="1"
          upperBound="1"/>
    </roles>
    <roles name="ring"
        entity="//@entities.8">
      <cardinality
          lowerBound="1"
          upperBound="1"/>
    </roles>
  </entities>
  <entities
      xsi:type="smalluml:Association"
      name="MusicBand">
    <roles name="singer"
        entity="//@entities.5">
      <cardinality
          lowerBound="1"
          upperBound="1"/>
    </roles>
    <roles name="musicians"
        entity="//@entities.6">
      <cardinality
          lowerBound="2"
          upperBound="*"/>
    </roles>
  </entities>
</smalluml:Diagram>
